import java.util.Scanner;
class main{
    public static void main(String args[]){
        System.out.println("Enter the number");
        int A[]=new int[6];
        Scanner s1= new Scanner(System.in);
        int i,sum=0;
        for(i=0;i<A.length;i++){
            A[i]=s1.nextInt();
            sum = sum+A[i];
        }
            for(i=0;i<A.length;i++){
                A[i]=sum-A[i];
                System.out.println(A[i]+" ");
            }
        }
    }
