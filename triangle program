import java.util.Scanner;
class Trianglesides{
    public static void main(String args[]){
        Scanner input = new Scanner(System.in);
        int a = input.nextInt();
        int b = input.nextInt();
        int c = input.nextInt();
        if(a+b>c && b+c>a && c+a>b){
            System.out.println("It Will Form an Triangle");
        }else{
            System.out.println("It Will Not Form an Triangle");
        }
    }
}
